{
    "name":"PCAP",
    "version":"1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "browser_action":{
        "default_title":"PCAP",
        "default_popup":"popup.html"
    },
    "background":{
        "scripts": ["background.js"]
    },
    "permissions": ["tabs"],

    "content_security_policy": "script-src 'self' https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js; object-src 'self';"
}

